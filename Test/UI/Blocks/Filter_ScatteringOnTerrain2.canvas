            "code" : "require Images;

operator ImageModifyGamma_task<<<index>>>(Color src[], io Color dst[], Float32 gammaR, Float32 gammaG, Float32 gammaB)
{
  Color c = src[index];
  if (c.r > 0 && c.r < 1)  c.r = pow(c.r, gammaR);
  if (c.g > 0 && c.g < 1)  c.g = pow(c.g, gammaG);
  if (c.b > 0 && c.b < 1)  c.b = pow(c.b, gammaB);
  dst[index] = c;
}

dfgEntry
{
  // init output.
  result = Image2DColor();
 
  // trivial case?
  if (gammaR == 1 && gammaG == 1 && gammaB == 1)
  {
    result = image;
    return;
  }

  // prepare output image.
  result.resize(image.width, image.height);
  
  // do it.
  Float32 gMin = 0.000001;
  Float32 gr = 1.0 / (gammaR > gMin ? gammaR : gMin);
  Float32 gg = 1.0 / (gammaG > gMin ? gammaG : gMin);
  Float32 gb = 1.0 / (gammaB > gMin ? gammaB : gMin);
  ImageModifyGamma_task<<<image.pixels.size()>>>(image.pixels, result.pixels, gr, gg, gb);
}
