    if (uIndex > 0)                surroundingPixels[0] = image.getPixel(uIndex - 1, vIndex - 1);
                                   surroundingPixels[1] = image.getPixel(uIndex,     vIndex - 1);
    if (uIndex + 1 < image.width)  surroundingPixels[2] = image.getPixel(uIndex + 1, vIndex - 1);
  }
  if (uIndex > 0)                  surroundingPixels[3] = image.getPixel(uIndex - 1, vIndex    );
  if (uIndex + 1 < image.width)    surroundingPixels[4] = image.getPixel(uIndex + 1, vIndex    );
  if (vIndex + 1 < image.height)
  {
    if (uIndex > 0)                surroundingPixels[5] = image.getPixel(uIndex - 1, vIndex + 1);
                                   surroundingPixels[6] = image.getPixel(uIndex,     vIndex + 1);
    if (uIndex + 1 < image.width)  surroundingPixels[7] = image.getPixel(uIndex + 1, vIndex + 1);
  }
