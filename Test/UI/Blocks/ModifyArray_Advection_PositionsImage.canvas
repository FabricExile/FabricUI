              {
                "objectType" : "ExecPort",
                "name" : "alignOrientation",
                "nodePortType" : "Out",
                "execPortType" : "In",
                "typeSpec" : "Scalar"
                },
              {
                "objectType" : "ExecPort",
                "name" : "lookAt",
                "nodePortType" : "Out",
                "execPortType" : "In",
                "typeSpec" : "Vec3"
                },
              {
                "objectType" : "ExecPort",
                "name" : "upVector",
                "nodePortType" : "Out",
                "execPortType" : "In",
                "typeSpec" : "Vec3"
