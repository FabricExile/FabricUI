              },
            "code" : "dfgEntry
{
  // init output.
  result = \"\";

  // add folder to result.
  result += folder;
  String lastChar = (folder.length() > 0 ? folder.subString(folder.length() - 1, 1) : \"\");
  if (lastChar != \"\\\\\" && lastChar != \"/\")
    result += \"/\";

  // count amount of '#' in filename.
  Count padding = filename.count(\"#\");

  // set result.
  if (padding > 0)  // with file index.
  {
    String split[] = filename.split(\"#\");
    result += split[0];

    String index = Integer(round(Float64(fileIndex)));
    while (index.length() < padding)
      index = \"0\" + index;
    result += index;

    for (Count i=1;i<split.size();i++)
      result += split[i];
  }
  else              // without file index.
  {
    result += filename;
  }

  // expand env vars.
  if (expandEnvVars)
    result = FilePath(result).expandEnvVars().string();
}
"
