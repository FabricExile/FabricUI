      "extDeps" : {
        "Geometry" : "*"
        },
      "presetGUID" : "AAB5B3FF9A4A54D460621DED1A4A9B7B",
      "nodes" : [
        {
          "objectType" : "Inst",
          "metadata" : {
            "uiGraphPos" : "{\"x\":462.0,\"y\":89.0}"
            },
          "name" : "applyGeneratePositionSet",
          "ports" : [
            {
              "objectType" : "InstPort",
              "name" : "exec",
              "nodePortType" : "IO"
              },
            {
              "objectType" : "InstPort",
              "name" : "mesh",
              "nodePortType" : "In"
              },
            {
              "objectType" : "InstPort",
              "name" : "ppa",
              "nodePortType" : "In",
              "defaultValues" : {
                "Float32" : 3
                }
              },
            {
              "objectType" : "InstPort",
              "name" : "positions",
              "nodePortType" : "Out"
              }
            ],
          "definition" : {
            "objectType" : "Func",
            "title" : "applyGeneratePositionSet",
            "ports" : [
              {
                "objectType" : "ExecPort",
                "name" : "exec",
                "nodePortType" : "IO",
                "execPortType" : "IO",
                "typeSpec" : "Execute"
                },
              {
                "objectType" : "ExecPort",
                "name" : "mesh",
                "nodePortType" : "Out",
                "execPortType" : "In",
                "typeSpec" : "PolygonMesh"
                },
              {
                "objectType" : "ExecPort",
                "name" : "ppa",
                "nodePortType" : "Out",
                "execPortType" : "In",
                "typeSpec" : "Scalar"
                },
              {
                "objectType" : "ExecPort",
                "name" : "positions",
                "nodePortType" : "In",
                "execPortType" : "Out",
                "typeSpec" : "Vec3[]"
                }
              ],
            "extDeps" : {
              "Geometry" : "*"
              },
            "code" : "dfgEntry
{
  // nothing to do?
  if (   ppa <= 0
      || mesh == null
      || mesh.polygonCount() <= 0
      || mesh.triangleCount() <= 0)
  {
    positions.resize(0);
    return;
  }

  // get the triangle list.
  UInt32 triangles[] = mesh.generateTriangleList();

  // count total amount of positions that will be generated.
  UInt32 count = 0;
  Float32 pointsPerSquareUnitSqrt = sqrt(ppa);
  for (UInt32 i=0;i<triangles.size();i+=3)
  {
    // get the three vertex positions of the current triangle.
    Vec3 v0 = mesh.getPointPosition(triangles[i + 0]);
    Vec3 v1 = mesh.getPointPosition(triangles[i + 1]);
    Vec3 v2 = mesh.getPointPosition(triangles[i + 2]);

    // calculate the triangle's surface area and base edge length.
    v1 -= v0;
    v2 -= v0;
    v0  = v1.cross(v2);
    Float32 sufaceArea     = 0.5f * v0.length();
    Float32 baseEdgeLength = v1.length();

    // pseudo \"draw\" the triangle and count the positions.
    Integer numY = (2 * sufaceArea / baseEdgeLength) * pointsPerSquareUnitSqrt;
    if (numY < 1)
      numY = 1;
    Float32 numYinv = 1.0 / Float32(numY);
    Float32 f       = baseEdgeLength * pointsPerSquareUnitSqrt;
    for (Integer y=0;y<numY;y++)
    {
      Float32 w2    = (Float32(y) + 0.5) * numYinv;
      Integer numX  = ceil((1 - w2) * f);
      if (numX < 1)
        numX = 1;
      count += numX;
    }
  }

  // resize and fill the output.
  positions.resize(count);
  count = 0;
  for (UInt32 i=0;i<triangles.size();i+=3)
  {
    // get the three vertex positions of the current triangle.
    Vec3 v0 = mesh.getPointPosition(triangles[i + 0]);
    Vec3 v1 = mesh.getPointPosition(triangles[i + 1]);
    Vec3 v2 = mesh.getPointPosition(triangles[i + 2]);

    // calculate the triangle's surface area and base edge length.
    v1 -= v0;
    v2 -= v0;
    v0  = v1.cross(v2);
    Float32 sufaceArea     = 0.5f * v0.length();
    Float32 baseEdgeLength = v1.length();

    // fill positions.
    Integer numY = (2 * sufaceArea / baseEdgeLength) * pointsPerSquareUnitSqrt;
    if (numY < 1)
      numY = 1;
    Float32 numYinv = 1.0 / Float32(numY);
    Float32 f       = baseEdgeLength * pointsPerSquareUnitSqrt;
    for (Integer y=0;y<numY;y++)
    {
      Float32 w2    = (Float32(y) + 0.5) * numYinv;
      Integer numX  = ceil((1 - w2) * f);
      if (numX < 1)
        numX = 1;
      Float32 numXinv = 1.0 / Float32(numX);
      for (Integer x=0;x<numX;x++,count++)
      {
        Float32 wA = (x + 0.5) * numXinv;
        Float32 w0 = (1 - w2)  *      wA;
        Float32 w1 = (1 - w2)  * (1 - wA);
        positions[count] =   w0 * mesh.getPointPosition(triangles[i + 0])
                           + w1 * mesh.getPointPosition(triangles[i + 1])
                           + w2 * mesh.getPointPosition(triangles[i + 2]);
      }
    }
  }
}
"
