        "extDeps" : {},
        "code" : "dfgEntry
{
  profile.resize(0);
  
  Vec3 p[];
  p.resize(0);
  
  Scalar x = - roadWidth
             - roadSidewalkBevelWidth
             - sidewalkBorderBevelWidth
             - sidewalkBorderWidth
             - sidewalkBorderInterWidth
             - sidewalkWidth
             - sidewalkBevelWidth;

  p.push(Vec3(x, 0.0, 0.0));
  p.push(Vec3(x, sidewalkHeight - sidewalkBevelWidth, 0.0));
  x += sidewalkBevelWidth;
  p.push(Vec3(x, sidewalkHeight, 0.0));
  x += sidewalkWidth;
  p.push(Vec3(x, sidewalkHeight, 0.0));
  x += 0.5 * sidewalkBorderInterWidth;
  p.push(Vec3(x, sidewalkHeight - 0.5 * sidewalkBorderInterWidth, 0.0));
  x += 0.5 * sidewalkBorderInterWidth;
  p.push(Vec3(x, sidewalkHeight, 0.0));
  x += sidewalkBorderWidth;
  p.push(Vec3(x, sidewalkHeight, 0.0));
  x += sidewalkBorderBevelWidth;
  p.push(Vec3(x, sidewalkHeight - sidewalkBorderBevelWidth, 0.0));
  p.push(Vec3(x, roadSidewalkBevelWidth, 0.0));
  x += roadSidewalkBevelWidth;
  p.push(Vec3(x, 0, 0.0));
  
  for (UInt32 i=0;i<p.size();i++)
    profile.push(p[i]);
  profile.push(Vec3(0.0, 0.0, 0.0));
  for (UInt32 i=0;i<p.size();i++)
  {
    Vec3 v = p[p.size() - 1 - i];
    v.x = -v.x;
    profile.push(v);
  }
}
