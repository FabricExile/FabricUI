            "definition" : {
              "objectType" : "Func",
              "title" : "Project Transforms",
              "ports" : [
                {
                  "objectType" : "Port",
                  "nodePortType" : "Out",
                  "name" : "bottom",
                  "execPortType" : "In",
                  "typeSpec" : "Xfo"
                  },
                {
                  "objectType" : "Port",
                  "nodePortType" : "Out",
                  "name" : "points",
                  "execPortType" : "In",
                  "typeSpec" : "Vec3[]"
                  },
                {
                  "objectType" : "Port",
                  "nodePortType" : "Out",
                  "name" : "count",
                  "execPortType" : "In",
                  "typeSpec" : "UInt32"
                  },
                {
                  "objectType" : "Port",
                  "nodePortType" : "In",
                  "name" : "result",
                  "execPortType" : "Out",
                  "typeSpec" : "Xfo[]"
                  },
                {
                  "objectType" : "Port",
                  "nodePortType" : "Out",
                  "name" : "animation",
                  "execPortType" : "In",
                  "typeSpec" : "Float32"
                  },
                {
                  "objectType" : "Port",
                  "nodePortType" : "Out",
                  "name" : "scale",
                  "execPortType" : "In",
                  "typeSpec" : "Vec3"
                  }
                ],
              "extDeps" : {},
              "code" : "dfgEntry {\n  result.resize(0);\n  if(count == 0 || points.size() < 2) {\n    return;\n  }\n  \n  Float32 length = 0.0;\n  for(Size i=0;i<points.size()-1;i++)\n    length += points[i].distanceTo(points[i+1]);\n  \n  Float32 step = length;\n  if(count > 1)\n    step = length / Float32(count-1);\n  Float32 travel = 0.0;\n  Float32 offset = (abs(animation % 1.0) * length) % step;\n  \n  UInt32 index = 0;\n  Float32 segment = points[index].distanceTo(points[index+1]);\n  while(offset > travel + segment)\n  {\n    index++;\n    if(index == points.size() - 1)\n      index = 0;\n    travel = (travel+segment) % length;\n    segment = points[index].distanceTo(points[index+1]); \n  }\n  \n  Float32 ratio = (offset - travel) / segment;\n  \n  while(true) {\n  \n    Xfo xfo = bottom;\n    xfo.sc = scale;\n    xfo.tr = points[index].linearInterpolate(points[index+1], ratio);\n    result.push(xfo);\n    \n    if(result.size() == count)\n      break;\n    \n    offset += step;\n    if(offset > travel + segment) {\n      index++;\n      if(offset > length) {\n        offset = offset % length;\n        index = 0;\n        travel = 0.0;\n      } else\n        travel = (travel+segment) % length;\n      segment = points[index].distanceTo(points[index+1]); \n    }\n    \n    ratio = (offset - travel) / segment;\n  }\n}\n"
              },
            "title" : "Project Transforms"
            },
