                "typeSpec" : "UInt32"
                },
              {
                "objectType" : "Port",
                "nodePortType" : "Out",
                "name" : "scaleRandomness",
                "execPortType" : "In",
                "typeSpec" : "Float32"
                },
              {
                "objectType" : "Port",
                "nodePortType" : "Out",
                "name" : "orientationRandomness",
                "execPortType" : "In",
                "typeSpec" : "Float32"
                },
              {
                "objectType" : "Port",
                "nodePortType" : "In",
                "name" : "xfo",
                "execPortType" : "Out",
                "typeSpec" : "Xfo[]"
                }
              ],
            "extDeps" : {
              "Geometry" : "*"
              },
            "code" : "dfgEntry\n{  \n  // nothing to do?\n  if (   numPositions <= 0\n      || mesh == null\n      || mesh.polygonCount() <= 0\n      || mesh.triangleCount() <= 0)\n  {\n    xfo.resize(0);\n    return;\n  }\n    \n  // build array of polygon indices that will be used for scattering.\n  UInt32 polyIndices[];\n  if (mode == 0)  // use all polygons.\n  {\n    polyIndices.resize(mesh.polygonCount());\n    for (UInt32 i=0;i<polyIndices.size();i++)\n      polyIndices[i] = i;\n  }\n  else\n  {\n    report(\"this mode is not yet implemented\");\n  }\n  \n  // no polygons to scatter from?\n  if (polyIndices.size() <= 0)\n  {\n    xfo.resize(0);\n    return;\n  }\n  \n  //\n  UInt32 rndPosIdx = 0;\n  UInt32 rndOriIdx = 0;\n  UInt32 rndSclIdx = 0;\n  xfo.resize(numPositions);\n  for (UInt32 i=0;i<numPositions;i++)\n  {\n    // pick a random polygon.\n    UInt32 polyIdx = floor(polyIndices.size() * mathRandomFloat32(seedPosition, rndPosIdx++));\n\n    // pick a random sub triangle.\n    UInt32 subTriangleIdx = 0;\n    if (mesh.getPolygonSubTriangleCount(polyIdx) > 1)\n      subTriangleIdx = floor(mesh.getPolygonSubTriangleCount(polyIdx) * mathRandomFloat32(seedPosition, rndPosIdx++));\n  \n    // get the triangle's vertex indices.\n    UInt32 subTriangleVertIdx[3] = mesh.getPolygonSubTrianglePolygonPoints(polyIdx, subTriangleIdx);\n\n    // calculate weights.\n    Float32 w[3];\n    for (UInt32 k=0;k<3;k++)\n    w[k] = 0.001 + mathRandomFloat32(seedPosition, rndPosIdx++);\n    \n    // normalize weights.\n    Float32 wSumInv = 1.0 / (w[0] + w[1] + w[2]);\n    for (UInt32 k=0;k<3;k++)\n    w[k] *= wSumInv;\n    \n    // get triangle positions.\n    Vec3 trianglePositions[3];\n    for (UInt32 k=0;k<3;k++)\n      trianglePositions[k] = mesh.getPointPosition(mesh.getPolygonPoint(polyIdx, subTriangleVertIdx[k]));\n\n    // set xfo's position.\n    xfo[i].tr.set(0.0);\n    for (UInt32 k=0;k<3;k++)\n      xfo[i].tr += w[k] * trianglePositions[k];\n    \n    // set xfo's orientation.\n    if (orientation == 1)\n    {\n      Vec3 direction(0);\n      for (UInt32 k=0;k<3;k++)  direction += w[k] * mesh.getPolygonPointNormal(polyIdx, subTriangleVertIdx[k]);\n      Vec3 upvector = trianglePositions[1] - trianglePositions[0];\n      if (orientationRandomness)\n      {\n        Quat q;\n        q.setFromAxisAndAngle(direction, orientationRandomness * 4.0 * 3.14159 * (-0.5 + mathRandomFloat32(seedOrientation, rndOriIdx++)));\n        upvector = q.rotateVector(upvector);\n      }\n      xfo[i].ori.setFromDirectionAndUpvector(direction, upvector);\n    }\n\n    // set xfo's scale.\n    if (scaleRandomness)  xfo[i].sc.set(scale * (1.0 + scaleRandomness * (-1.0 + 2.0 * mathRandomFloat32(seedScale, rndSclIdx++))));\n    else                  xfo[i].sc.set(scale);\n  }\n}\n"
            },
          "title" : "applyScatterXfo"
          },
