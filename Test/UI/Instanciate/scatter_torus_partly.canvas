                },
              {
                "objectType" : "Port",
                "nodePortType" : "Out",
                "name" : "orientation",
                "execPortType" : "In",
                "typeSpec" : "UInt8"
                },
              {
                "objectType" : "Port",
                "nodePortType" : "Out",
                "name" : "numPositions",
                "execPortType" : "In",
                "typeSpec" : "UInt32"
                },
              {
                "objectType" : "Port",
                "nodePortType" : "Out",
                "name" : "polygonIndices",
                "execPortType" : "In",
                "typeSpec" : "UInt32[]"
                },
              {
                "objectType" : "Port",
