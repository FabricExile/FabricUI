          "definition" : {
            "objectType" : "Func",
            "title" : "applyScatterXfo",
            "ports" : [
              {
                "objectType" : "Port",
                "nodePortType" : "IO",
                "name" : "exec",
                "execPortType" : "IO",
                "typeSpec" : "Execute"
                },
              {
                "objectType" : "Port",
