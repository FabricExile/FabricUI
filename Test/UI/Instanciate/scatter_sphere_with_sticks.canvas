          "definition" : {
            "objectType" : "Func",
            "title" : "applyScatterXfo",
            "ports" : [
              {
                "objectType" : "Port",
                "nodePortType" : "Out",
                "name" : "mesh",
                "execPortType" : "In",
                "typeSpec" : "PolygonMesh"
                },
              {
