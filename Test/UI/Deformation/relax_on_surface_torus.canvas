          "definition" : {
            "objectType" : "Func",
            "title" : "applySurfaceArea",
            "ports" : [
              {
                "objectType" : "Port",
                "nodePortType" : "IO",
                "name" : "exec",
                "execPortType" : "IO",
                "typeSpec" : "Execute"
                },
              {
                "objectType" : "Port",
                "nodePortType" : "Out",
                "name" : "mesh",
                "execPortType" : "In",
                "typeSpec" : "PolygonMesh"
                },
              {
                "objectType" : "Port",
                "nodePortType" : "In",
                "name" : "result",
                "execPortType" : "Out",
                "typeSpec" : "Float64"
                }
              ],
            "extDeps" : {
              "Geometry" : "*"
              },
            "code" : "dfgEntry
{
  // init output.
  result = 0;
  
  // nothing to calculate?
  if (   mesh == null
      || mesh.polygonCount() <= 0
      || mesh.triangleCount() <= 0)
  {
    return;
  }

  // calculate surface area.
  UInt32 triangles[] = mesh.generateTriangleList();
  for (UInt32 i=0;i<triangles.size();i+=3)
  {
    // get the three vertex positions of the current triangle.
    Vec3 v0 = mesh.getPointPosition(triangles[i + 0]);
    Vec3 v1 = mesh.getPointPosition(triangles[i + 1]);
    Vec3 v2 = mesh.getPointPosition(triangles[i + 2]);

    // calculate the triangle's surface area and base edge length.
    v1 -= v0;
    v2 -= v0;
    v0  = v1.cross(v2);
    result += 0.5 * v0.length();
  }
}
"
            }
