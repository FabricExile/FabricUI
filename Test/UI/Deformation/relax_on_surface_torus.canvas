            "code" : "operator applyRelaxOnSurfaceVec3_task<<<index>>>(PolygonMesh polymesh, Points points, Float32 cutoffDistance, Float32 strengthRelax, Float32 strengthReproject, io Vec3 res[])
{
  // relax.
  if (strengthRelax > 0)
  {
    // begin spatial query.
    Ref<SpatialQuery> query = points.beginSpatialQuery();
    
    // get the neighbor elements and calculate new position.
    Size num = query.getElementsInBSphere(res[index], cutoffDistance);    
    if (num > 1)
    {
      Vec3 dir(0);
      for (Size i=0;i<num;i++)
      {
        Vec3    v = res[index] - points.getPosition(query.getQueriedElement(i));
        Float32 f = 1.0 - v.length() / cutoffDistance;
        dir += f * f * v;
      }
      res[index] += strengthRelax * dir;
    }
      
    // end spatial query.
    points.endSpatialQuery(query);
  }

  // constrain the result to the surface of the mesh.
  if (strengthReproject > 0)
  {
    // begin spatial query.
    Ref<SpatialQuery> query = polymesh.beginSpatialQuery();
    
    // constrain.
    GeometryLocation gloc = query.getClosest(res[index], Vec3(1, 1, 1), SCALAR_INFINITE);
    if (gloc.isValid())
    {
      if (strengthReproject >= 1) res[index]  = polymesh.getPositionAtLocation(gloc);
      else                        res[index] += strengthReproject * (polymesh.getPositionAtLocation(gloc) - res[index]);
    }
      
    // end spatial query.
    polymesh.endSpatialQuery(query);
  }
}

dfgEntry
{
  // resize and init output.
  result.resize(positions.size());
  for (UInt32 i=0;i<result.size();i++)
    result[i] = positions[i];
  
  // nothing to do?
  if (   mesh == null
      || mesh.polygonCount()  <= 0
      || mesh.triangleCount() <= 0
      || cutoffDistance       <= 0
      || iterations           <= 0)  return;
  if (   strengthRelax        <= 0
      && strengthReproject    <= 0)  return;
  
  // prepare mesh for spatial queries.
  GenericValueContainer options = GenericValueContainer();
  PrepareForSpatialQueries_setOctree(options);
  if (strengthReproject > 0)
    mesh.prepareForSpatialQueries(result.size(), options);
    
  // create and init the Points object.
  Points points = Points();
  points.defaultPointSize = 0;
  if (strengthRelax > 0)
    points.addPoints(result.size());
    
  // do it.
  for (Integer i=0;i<iterations;i++)
  {
    // set positions in points from current result.
    if (strengthRelax > 0)
    {
      for (UInt32 j=0;j<result.size();j++)
        points.setPosition(j, result[j]);
      points.incrementPositionsVersion();
    }

    // prepare points for spatial queries.
    if (strengthRelax > 0)
    {
      GenericValueContainer options = GenericValueContainer();
      PrepareForSpatialQueries_setOctree(options);
      points.prepareForSpatialQueries(result.size(), options);
    }
  
    // relax and reproject.
    applyRelaxOnSurfaceVec3_task<<<result.size()>>>(mesh, points, cutoffDistance, strengthRelax, strengthReproject, result);
  }
}
