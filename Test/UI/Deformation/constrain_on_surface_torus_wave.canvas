            "code" : "operator applyConstrainToSurfaceVec3_task<<<index>>>(PolygonMesh  polymesh, Vec3 pos[], Scalar amount, io Vec3 res[])
{
  // begin spatial query.
  Ref<SpatialQuery> query = polymesh.beginSpatialQuery();

  // get closest location and calculate result.
  GeometryLocation gloc = query.getClosest(pos[index], Vec3(1, 1, 1), SCALAR_INFINITE);
  if (gloc.isValid())
  {
    if (amount == 1)  res[index] = polymesh.getPositionAtLocation(gloc);
    else              res[index] = pos[index] + amount * (polymesh.getPositionAtLocation(gloc) - pos[index]);
  }
  else
  {
    res[index] = pos[index];
  }

  // end spatial query.
  polymesh.endSpatialQuery(query);
}

dfgEntry
{
  // resize output.
  result.resize(positions.size());
  
  // do it.
  if (amount == 0)
  {
    // copy positions into result.
    for (UInt32 i=0;i<result.size();i++)
      result[i] = positions[i];
  }
  else
  {
    // prepare mesh for spatial queries.
    GenericValueContainer options = GenericValueContainer();
    PrepareForSpatialQueries_setOctree(options);
    mesh.prepareForSpatialQueries(result.size(), options);
    
    // constrain.
    applyConstrainToSurfaceVec3_task<<<result.size()>>>(mesh, positions, amount, result);
  }
}
