{
  Vec3 v = c + sf * pos[index];
  
  res[index].x = pos[index].x + a.x * perlinNoise(v.x, v.z, v.y);
  res[index].y = pos[index].y + a.y * perlinNoise(v.y, v.x, v.z);
  res[index].z = pos[index].z + a.z * perlinNoise(v.z, v.y, v.x);
}

