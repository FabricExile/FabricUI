            "code" : "require Geometry;

operator PolygonMeshDeformPushAll_task<<<index>>>(io PolygonMesh mesh, Float32 length)
{
  mesh.setPointPosition(index, mesh.getPointPosition(index) + length * mesh.getPointNormal(index));
}

operator PolygonMeshDeformPushRegion_task<<<index>>>(io PolygonMesh mesh, Float32 length, Vec3 center, Float32 distance)
{
  // get i-th point position and normal.
  Vec3 p = mesh.getPointPosition(index);
  Vec3 n = mesh.getPointNormal(index);

  // calculate vector from center to p.
  Vec3 v = p - center;

  // calculate amount of pushing.
  Float32 f = length *  (1.0 - v.length() / distance);
   
  // push amount greater than zero?
  if (f > 0)
  {
    // calculate and set new point position.
    p += f * n;
    mesh.setPointPosition(index, p);
  }
}

dfgEntry
{
  // nothing to do?
  if (length == 0)
    return;
  if (useCenterDistance && distance <= 0)
    return;
    
  // do it.
  if (useCenterDistance)  PolygonMeshDeformPushRegion_task<<<mesh.pointCount()>>>(mesh, length, center, distance);
  else                    PolygonMeshDeformPushAll_task<<<mesh.pointCount()>>>(mesh, length);

  // increment version.
  mesh.incrementPointPositionsVersion();
}
