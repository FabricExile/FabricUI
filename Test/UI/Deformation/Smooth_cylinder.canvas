            "code" : "require Geometry;

operator PolygonMeshDeformSmooth_smooth_task<<<index>>>(PolygonMesh mesh, Vec3 src<>, io Vec3 dst<>, Float32 strength, Boolean preserveBoundaries)
{
  if (preserveBoundaries && mesh.getPointPolygonCount(index) > 0)
  {
    Size    wingPolygonPointIndexBegin;
    Size    wingPolygonPointIndexEnd;
    Boolean isClosedWing;
    mesh.getPointPolygonFullBorderInfo(index, 0, wingPolygonPointIndexBegin, wingPolygonPointIndexEnd, isClosedWing);
    if (!isClosedWing)
    {
      dst[index] = src[index];
      return;
    }
  }
  
  LocalL16UInt32Array surroundingPoints;
  mesh.getPointSurroundingPoints(index, false, surroundingPoints);
  Vec3 newPosition(0, 0, 0);
  if (surroundingPoints.size() > 1)
  {
    for (UInt32 j=0;j<surroundingPoints.size();j++)
      newPosition += src[surroundingPoints.get(j)];
    newPosition /= surroundingPoints.size();
  }
  else if (surroundingPoints.size() == 1)
  {
    newPosition = 0.5 * (src[index] + src[surroundingPoints.get(0)]);
  }
  else
  {
    newPosition = src[index];
  }
  
  if (strength == 1)  dst[index] = newPosition;
  else                dst[index] = src[index].linearInterpolate(newPosition, strength);
}

operator PolygonMeshDeformSmooth_reproject_task<<<index>>>(PolygonMesh mesh, io Vec3 positions<>, Float32 strength)
{
  // begin spatial query.
  Ref<SpatialQuery> query = mesh.beginSpatialQuery();

  // calculate.
  GeometryLocation gloc = query.getClosest(positions[index], Vec3(1, 1, 1), SCALAR_INFINITE);
  if (gloc.isValid())
  {
    if (strength == 1)  positions[index] = mesh.getPositionAtLocation(gloc);
    else                positions[index] = positions[index].linearInterpolate(mesh.getPositionAtLocation(gloc), strength);
  }

  // end spatial query.
  mesh.endSpatialQuery(query);
}

dfgEntry
{
  // nothing to do?
  if (!enable || mesh == null || mesh.pointCount() == 0)
    return;

  // smooth.
  if (smoothStrength > 0)
  {
    // create the two Vec3 arrays that will be used to calculate the smoothed positions.
    Vec3 positions0[];
    Vec3 positions1[];
    positions0.resize(mesh.pointCount());
    positions1.resize(mesh.pointCount());
    for (UInt32 i=0;i<mesh.pointCount();i++)
      positions0[i] = mesh.getPointPosition(i);

    // split the smooth strength.
    UInt32  smoothStrengthIter = floor(smoothStrength);
    Float32 smoothStrengthFrac = smoothStrength - smoothStrengthIter;

    // init last dst flag.
    Integer lastDst = 0;

    // prepare for spatial queries.
    if (reprojectStrength > 0)
    {
      GenericValueContainer options = GenericValueContainer();
      PrepareForSpatialQueries_setOctree(options);
      mesh.prepareForSpatialQueries(mesh.pointCount(), options);
    }

    // perform the full smooth iterations.
    for (UInt32 it=0;it<smoothStrengthIter;it++)
    {
      Vec3 src<>;
      Vec3 dst<>;
      if (lastDst == 0)
      {
        src = positions0;
        dst = positions1;
        lastDst = 1;
      }
      else
      {
        src = positions1;
        dst = positions0;
        lastDst = 0;
      }
      PolygonMeshDeformSmooth_smooth_task<<<mesh.pointCount()>>>(mesh, src, dst, 1.0, preserveBoundaries);
      if (reprojectStrength > 0 && reprojectMode != 0)
        PolygonMeshDeformSmooth_reproject_task<<<mesh.pointCount()>>>(mesh, dst, reprojectStrength);
    }

    // perform the fractional smooth.
    if (smoothStrengthFrac > 0)
    {
      Vec3 src<>;
      Vec3 dst<>;
      if (lastDst == 0)
      {
        src = positions0;
        dst = positions1;
        lastDst = 1;
      }
      else
      {
        src = positions1;
        dst = positions0;
        lastDst = 0;
      }
      PolygonMeshDeformSmooth_smooth_task<<<mesh.pointCount()>>>(mesh, src, dst, smoothStrengthFrac, preserveBoundaries);
      if (reprojectStrength > 0 && reprojectMode != 0)
        PolygonMeshDeformSmooth_reproject_task<<<mesh.pointCount()>>>(mesh, dst, reprojectStrength);
    }

    // perform last reprojection if needed.
    if (reprojectStrength > 0 && reprojectMode == 0)
    {
      Vec3 dst<>;
      if (lastDst == 0)  dst = positions0;
      else               dst = positions1;
      PolygonMeshDeformSmooth_reproject_task<<<mesh.pointCount()>>>(mesh, dst, reprojectStrength);
    }

    // get the scalar attribute (or null) for the last step.
    Ref<ScalarAttribute> attr = null;
    if (useScalarAttribute)
    {
      attr = mesh.getAttribute(scalarAttribute, ScalarAttribute);
      if (attr == null)
        report(\"[Deform.Smooth] WARNING: the input mesh has no scalar attribute called \\\"\" + scalarAttribute + \"\\\"\");
    }
    
    // last step: set the new positions.
    Vec3 dst<>;
    if (lastDst == 0)  dst = positions0;
    else               dst = positions1;
    if (attr == null)  for (UInt32 i=0;i<mesh.pointCount();i++)  mesh.setPointPosition(i, dst[i]);
    else               for (UInt32 i=0;i<mesh.pointCount();i++)  mesh.setPointPosition(i, mesh.getPointPosition(i).linearInterpolate(dst[i], attr.values[mesh.getPointAttributeIndex(i)]));

    // increment version.
    mesh.incrementPointPositionsVersion();
  }

  // recomputer normals.
  if (recomputeNormals)
  {
    mesh.recomputePointNormals(hardAngleRadians);
  }
}
