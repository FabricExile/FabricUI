    // begin spatial query.
    Ref<SpatialQuery> query = points.beginSpatialQuery();
    
    // get the neighbor elements and calculate new position.
    Size num = query.getElementsInBSphere(res[index], cutoffDistance);    
    if (num > 1)
    {
      Vec3 dir(0);
      for (Size i=0;i<num;i++)
      {
        Vec3    v = res[index] - points.getPosition(query.getQueriedElement(i));
        Float32 f = 1.0 - v.length() / cutoffDistance;
        dir += f * f * v;
      }
      res[index] += strengthRelax * dir;
    }
      
    // end spatial query.
    points.endSpatialQuery(query);
  }

  // constrain the result to the surface of the mesh.
  if (strengthReproject > 0)
  {
    // begin spatial query.
    Ref<SpatialQuery> query = polymesh.beginSpatialQuery();
    
    // constrain.
    GeometryLocation gloc = query.getClosest(res[index], Vec3(1, 1, 1), SCALAR_INFINITE);
    if (gloc.isValid())
    {
      if (strengthReproject >= 1) res[index]  = polymesh.getPositionAtLocation(gloc);
      else                        res[index] += strengthReproject * (polymesh.getPositionAtLocation(gloc) - res[index]);
    }
      
    // end spatial query.
    polymesh.endSpatialQuery(query);
  }
