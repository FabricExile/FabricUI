        "extDeps" : {},
        "code" : "operator nbody<<<index>>>(Float32 G, Vec3 p[], io Vec3 a[]) {
  Vec3 pp = p[index];
  Vec3 aa();
  for ( Index i = 0; i < p.size(); ++i )
  {
    if ( i != index )
    {
      Vec3 diff = p[i] - pp;
      Float32 distSq = diff.lengthSquared();
      if ( distSq >= 1e-6 )
        aa += (G / distSq) * diff;
    }
  }
  a[index] = aa;
}
 
