        "extDeps" : {},
        "code" : "dfgEntry
{
  Count count = numPoints;

  if (points.elementCount() >= count)
    return;

  Index newPointIndex = points.addPoints(count);
  
  GeometryAttributes geometryAttributes    = points.getAttributes();
  Vec3Attribute      positionAttribute     = geometryAttributes.getOrCreateVec3Attribute  ('positions');
  ScalarAttribute    sizeAttribute         = geometryAttributes.getOrCreateScalarAttribute('pointSizes');
  ColorAttribute     colorAttribute        = geometryAttributes.getOrCreateColorAttribute ('vertexColors');
  Vec3Attribute      velocityAttribute     = geometryAttributes.getOrCreateVec3Attribute  ('velocities');
  Vec3Attribute      accelerationAttribute = geometryAttributes.getOrCreateVec3Attribute  ('accelerations');
  
  UInt32  rnd = 0;
  for ( Index i = 0; i < count; ++i )
  {
    sizeAttribute        .values[newPointIndex+i] = 2;
    colorAttribute       .values[newPointIndex+i] = Color(1.0,0.0,0.0,1.0);
    accelerationAttribute.values[newPointIndex+i] = Vec3();

    // position.
    Vec3 p;
    Float32 a = 2.0 * 3.14 * mathRandomScalar(0, rnd++);
    p.x = radius1 * sin(a) + radius2 * mathRandomScalar(0, rnd++);
    p.y =              0.1 * radius2 * mathRandomScalar(0, rnd++);
    p.z = radius1 * cos(a) + radius2 * mathRandomScalar(0, rnd++);
    positionAttribute.values[newPointIndex+i] = p;

    // velocity.
    Vec3 v;
    v.x = -p.z;
    v.y =  0;
    v.z =  p.x;
    v *= initialSpeed / v.length();
    velocityAttribute.values[newPointIndex+i] = v;
  }
}
