  io Vec3  p[],
  io Vec3  v[],
  in Vec3  a[],
  io Color c[],
  in Float32 simulationSpeed
  )
{
  v[i] += simulationSpeed * a[i];
  p[i] += simulationSpeed * v[i];
  
  Float32 aLen = a[i].length();
  c[i].r = 0.9 + 0.09 * aLen;
  c[i].g =       0.09 * aLen;
  c[i].b =       0.06 * aLen;
}

