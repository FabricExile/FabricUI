              "code" : "operator linesPosUpdate_task<<<index>>>(Vec3 pos[], UInt32 pointsPerStrand, io Lines lines)
{
  UInt32 idx = (index + 1) * pointsPerStrand - 1;
  for (UInt32 j=0;j<pointsPerStrand-1;j++)
  {
    lines.setPosition(idx, lines.getPosition(idx - 1));
    idx--;
  }
  lines.setPosition(idx, pos[index]);
}

dfgEntry
{
  // is lines null?
  if (!lines)
    lines = Lines();

  // nothing to do?
  if (pointsPerStrand < 2 || points.elementCount() <= 0)
  {
    lines.clear();
    return;
  }

  // reset?
  if (reset || lines.pointCount() != pointsPerStrand * points.elementCount())
    lines.clear();

  // get point positions.
  GeometryAttributes attrs = points.getAttributes();
  Vec3Attribute      pos   = attrs.getOrCreateVec3Attribute('positions');

  // first time?
  if (lines.pointCount() == 0)
  {
    UInt32 idx;

    // positions.
    lines.addPoints(pointsPerStrand * points.elementCount());
    idx = 0;
    for (UInt32 i=0;i<points.elementCount();i++)
      for (UInt32 j=0;j<pointsPerStrand;j++)
      {
        lines.setPosition(idx++, pos.values[i]);
      }
    
    // lines.
    idx = 0;
    for (UInt32 i=0;i<points.elementCount();i++)
    {
      lines.addLine(idx, idx + pointsPerStrand - 1);
      idx += pointsPerStrand;
    }
    
    // done.
    return;
  }
  
  // update lines' positions.
  linesPosUpdate_task<<<points.elementCount()>>>(pos.values, pointsPerStrand, lines);
  lines.incrementPositionsVersion();
}
