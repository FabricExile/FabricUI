        "code" : "dfgEntry
{
  
// get point positions.
GeometryAttributes attrs = points.getAttributes();
Vec3Attribute      pos   = attrs.getOrCreateVec3Attribute('positions');
IntegerAttribute   conn   = attrs.getOrCreateIntegerAttribute('connections');
ScalarAttribute    size   = attrs.getOrCreateScalarAttribute('pointSizes');

lines = Lines();
lines.addPoints(points.elementCount()*points.elementCount());


UInt32 idx=0;

// reset connections
for (UInt32 i=0;i<points.elementCount();i++)
{
  conn.values[i] = 0;
  size.values[i] = 2.0;
}

for (UInt32 i=0;i<points.elementCount();i++)
{
  for (UInt32 j=i+1;j<points.elementCount();j++)
  {
    if (conn.values[j] >= maxconnections) continue;
    Vec3 oldPart = pos.values[i];
    Vec3 newPart = pos.values[j];
    Scalar dx = oldPart.x  - newPart.x;
    Scalar dy = oldPart.y  - newPart.y;
    Scalar dz = oldPart.z  - newPart.z;
    Scalar dist = sqrt( dx * dx + dy * dy + dz * dz );
    

    if ( dist < minDistance ) 
    {
      //Make the point particles bigger and increase their connection number
      size.values[i] += 1.0;
      size.values[j] += 1.0;
      conn.values[i] +=1;
      conn.values[j] +=1;
      
      lines.setPosition(idx, oldPart);
      lines.setPosition(idx+1, newPart);
      lines.addLine(idx, idx+1);
      idx+=2;
    }
  }
}

  lines.incrementPositionsVersion();
  conn.incrementVersion();
  size.incrementVersion();
}"
