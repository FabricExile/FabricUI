        "code" : "dfgEntry
{
  Count count = numPoints;

  if (points.elementCount() >= count)
    return;

  Index newPointIndex = points.addPoints(count);
  
  GeometryAttributes geometryAttributes    = points.getAttributes();
  Vec3Attribute      positionAttribute     = geometryAttributes.getOrCreateVec3Attribute   ('positions');
  ColorAttribute     colorAttribute        = geometryAttributes.getOrCreateColorAttribute  ('vertexColors');
  IntegerAttribute   connectionsAttribute  = geometryAttributes.getOrCreateIntegerAttribute('connections');
  ScalarAttribute    sizeAttribute         = geometryAttributes.getOrCreateScalarAttribute ('pointSizes');

  
  UInt32  rnd = 0;
  for ( Index i = 0; i < count; ++i )
  {

    sizeAttribute        .values[newPointIndex+i] = 15.0;
      
    colorAttribute       .values[newPointIndex+i] = Color(0.0, mathRandomScalar(0, rnd++),mathRandomScalar(0, rnd++),1.0);
    
    Vec3 p;
    Float32 a = 2.0 * 3.14 * mathRandomScalar(0, rnd++);
    p.x = radius * mathRandomScalar(0, rnd++);
    p.y = radius * mathRandomScalar(0, rnd++);
    p.z = radius * mathRandomScalar(0, rnd++);
    positionAttribute.values[newPointIndex+i] = p;

    connectionsAttribute.values[newPointIndex+i] = 0;
  }
  positionAttribute.incrementVersion();
  colorAttribute.incrementVersion();
  sizeAttribute.incrementVersion();
  connectionsAttribute.incrementVersion();
}
