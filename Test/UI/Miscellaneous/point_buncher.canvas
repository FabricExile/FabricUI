        "extDeps" : {},
        "code" : "
operator applyTurbulizeVec3_task<<<index>>>(io Vec3 pos[], Vec3 c, Vec3 a, Vec3 sf)
{
  Vec3 v = c + sf * pos[index];
  
  pos[index].x = pos[index].x + a.x * perlinNoise(v.x, v.z, v.y);
  pos[index].y = pos[index].y + a.y * perlinNoise(v.y, v.x, v.z);
  pos[index].z = pos[index].z + a.z * perlinNoise(v.z, v.y, v.x);
  
}

dfgEntry
{  
  Vec3 c = center + time * timeFrequency;
  

  GeometryAttributes attrs = points.getAttributes();
  Vec3Attribute posAttr = attrs.getOrCreateVec3Attribute('positions');
    
  applyTurbulizeVec3_task<<<posAttr.values.size()>>>(posAttr.values, c, amplitude, spaceFrequency);
  posAttr.incrementVersion();
}
