dfgEntry {
  type = \"\";
  if(attributeName == \"\")
  {
    report(\"Error: empty attribute name in \\\"GetValueAtLocation\\\"\");
    return;
  }
  if(!location.isValid())
    return;

  BaseGeometry baseGeom = geometry;
  if(baseGeom == null)
  {
    report(\"Error: missing geometry in \\\"GetValueAtLocation\\\"\");
    return;
  }

  GeometryAttributes attributes = baseGeom.attributes;
  GeometryAttribute attr = attributes.getAttribute(attributeName);
  if(attr == null)
    return;

  switch(attr.type())
  {
    case IntegerAttribute:
    {
      IntegerAttribute cast = attr;
      type = \"SInt32\";
      sint32Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case UInt32Attribute:
    {
      UInt32Attribute cast = attr;
      type = \"UInt32\";
      uint32Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case ScalarAttribute:
    {
      ScalarAttribute cast = attr;
      type = \"Float32\";
      float32Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case Vec2Attribute:
    {
      Vec2Attribute cast = attr;
      type = \"Vec2\";
      vec2Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case Vec3Attribute:
    {
      Vec3Attribute cast = attr;
      type = \"Vec3\";
      vec3Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case Vec4Attribute:
    {
      Vec4Attribute cast = attr;
      type = \"Vec4\";
      vec4Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case QuatAttribute:
    {
      QuatAttribute cast = attr;
      type = \"Quat\";
      quatValue = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case Mat33Attribute:
    {
      Mat33Attribute cast = attr;
      type = \"Mat33\";
      mat33Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case Mat44Attribute:
    {
      Mat44Attribute cast = attr;
      type = \"Mat44\";
      mat44Value = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case ColorAttribute:
    {
      ColorAttribute cast = attr;
      type = \"Color\";
      colorValue = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case RGBAttribute:
    {
      RGBAttribute cast = attr;
      type = \"RGB\";
      rgbValue = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case RGBAAttribute:
    {
      RGBAAttribute cast = attr;
      type = \"RGBA\";
      rgbaValue = GetAttributeAtLocation(geometry, location, cast);
      break;
    }
    case UInt16ConstantArrayAttribute:
    {
      UInt16ConstantArrayAttribute cast = attr;
      type = \"UInt16[]\";
      LocalL16UInt16Array value = GetAttributeAtLocation(geometry, location, cast);
      uint16ArrayValue.resize(value.size());
      for(Size j=0;j<value.size();j++)
        uint16ArrayValue[j] = value.get(j);
