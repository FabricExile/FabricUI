              "code" : "dfgEntry
{
  // init output.
  Xfo xfo[];
  
  // invalid combination of inputs?
  if (tags.size() != mesh.pointCount())
    return;

  // process indices in tagThese[].
  for (UInt32 i=0;i<tagThese.size();i++)
  {
    if (tagThese[i] < tags.size())
      tags[tagThese[i]] = 1.0;
  }

  // count tags with a value greater than zero.
  UInt32 countTagsGtZero = 0;
  for (UInt32 i=0;i<tags.size();i++)
    if (tags[i] > 0)
      countTagsGtZero++;
  
  // nothing to do?
  if (countTagsGtZero == 0)
    return;

  // grow the tags.
  Float32 t[];
  t.resize(tags.size());
  for (UInt32 i=0;i<tags.size();i++)
    t[i] = tags[i];
  LocalL16UInt32Array neighIdx;
  Vec3 turbCenter = Vec3(Float32(frame) * timeFrequency);
  for (UInt32 i=0;i<tags.size();i++)
  {
    if (t[i] < 1)
    {
      mesh.getPointSurroundingPoints(i, false, neighIdx);
      Float32 tSum = 0;
      if (neighIdx.count <= 16)
      {
        for (UInt32 j=0;j<neighIdx.count;j++)
          tSum += t[neighIdx.first16[j]];
      }
      else
      {
        for (UInt32 j=0;j<16;j++)
          tSum += t[neighIdx.first16[j]];
        for (UInt32 j=0;j<neighIdx.count-16;j++)
          tSum += t[neighIdx.others[j]];
      }
      if (tSum > 0)
      {
        //
        Vec3 v = turbCenter + spaceFrequency * mesh.getPointPosition(i);
        Float32 gs = growSpeed * abs(perlinNoise(v.x, v.y, v.z));
      
        //
        tags[i] += gs * tSum;
        if (tags[i] > 1)  tags[i] = 1;
      }
    }
  }

  // re-count tags with a value greater than xfoTagCutoff.
  UInt32 countTagsGtXfoTagCutoff = 0;
  for (UInt32 i=0;i<tags.size();i++)
    if (tags[i] > xfoTagCutoff)
      countTagsGtXfoTagCutoff++;
    
  // set ouput.
  xfo.resize(countTagsGtXfoTagCutoff);
  UInt32 xfoIdx = 0;
  for (UInt32 i=0;i<tags.size();i++)
  {
    if (tags[i] > xfoTagCutoff)
    {
      xfo[xfoIdx].tr = mesh.getPointPosition(i);
      xfo[xfoIdx].sc.set(tags[i]);
      xfoIdx++;
    }
  }
}
"
