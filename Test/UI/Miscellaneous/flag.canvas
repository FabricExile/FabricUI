        "code" : "operator satisfy<<<index>>>(
  Vec3 p[],
  io Vec3 v[],
  Integer f[],
  UInt32 cols,
  UInt32 rows,
  Float32 colWidthSq,
  Float32 rowHeightSq,
  Float32 s
  )
{
  UInt32 row = index / (cols + 1);
  UInt32 col = index % (cols + 1);
  
  if ( !f[index] )
  {
    if ( row > 0 )
    {
      Vec3 delta = p[(row - 1) * (cols + 1) + col] - p[index];
      Float32 deltaLenSq = delta.lengthSquared();
      Float32 diff = (deltaLenSq - rowHeightSq) / (rowHeightSq + deltaLenSq) * s;
      v[index] += delta * diff;
    }
    
    if ( row < rows )
    {
      Vec3 delta = p[(row + 1) * (cols + 1) + col] - p[index];
      Float32 deltaLenSq = delta.lengthSquared();
      Float32 diff = (deltaLenSq - rowHeightSq) / (rowHeightSq + deltaLenSq) * s;
      v[index] += delta * diff;
    }
    
    if ( col > 0 )
    {
      Vec3 delta = p[row * (cols + 1) + col - 1] - p[index];
      Float32 deltaLenSq = delta.lengthSquared();
      Float32 diff = (deltaLenSq - colWidthSq) / (colWidthSq + deltaLenSq) * s;
      v[index] += delta * diff;
    }
    
    if ( col < cols )
    {
      Vec3 delta = p[row * (cols + 1) + col + 1] - p[index];
      Float32 deltaLenSq = delta.lengthSquared();
      Float32 diff = (deltaLenSq - colWidthSq) / (colWidthSq + deltaLenSq) * s;
      v[index] += delta * diff;
    }
  }
}

