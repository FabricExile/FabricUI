{
  // woops?
  if (pm == null)
  {
    report(\"Error: null!\");
    return;
  }

  // adjust the amount of rows and columns.
  Count adjRows = rows;
  Count adjCols = cols;
  if (adjRows < 1)  adjRows = 1;
  if (adjCols < 1)  adjCols = 1;

  // leave early if doNothing is 'true' and if the current topology matches the amount of rows and columns.
  if (doNothing && adjRows * adjCols == pm.polygonCount())
    return;
      
  // clear the mesh.
  pm.clear();

  // build the mesh.
  Scalar width  = adjCols;
  Scalar height = adjRows;
  pm.beginStructureChanges();
  pm.createPoints( (adjRows + 1) * (adjCols + 1) );
  for (Index row = 0; row <= adjRows; ++row)
  {
    for (Index col = 0; col <= adjCols; ++col)
    {
       Index index = row * (adjCols + 1) + col;
       pm.setPointPosition(index, Vec3(
         width  * col / adjCols,
         height * row / adjRows,
         0
         ));
       pm.setPointNormal(index, Vec3( 0, 0, 1 ));
    }
  }
  for (Index row = 0; row < adjRows; ++row)
  {
    for (Index col = 0; col < adjCols; ++col)
    {
      pm.addPolygon(
        row * (adjCols + 1) + col,
        (row + 1) * (adjCols + 1) + col,
        (row + 1) * (adjCols + 1) + col + 1,
        row * (adjCols + 1) + col + 1,
        );
      pm.updatePointsPolygonOrder();
    }
  }
  pm.endStructureChanges();
  
  GeometryAttributes attrs = pm.getAttributes();
  Ref<Vec2Attribute> uvs = attrs.getOrCreateVec2Attribute('uvs0');
  for (Index row = 0; row <= adjRows; ++row)
  {
    for (Index col = 0; col <= adjCols; ++col)
    {
       Index index = row * (adjCols + 1) + col;
       uvs.values[index] = Vec2(Float32(col) / adjCols, Float32(row) / adjRows);
    }
  }
  
  Vec3Attribute v = attrs.getOrCreateVec3Attribute('velocities');
  IntegerAttribute fixedAttr = attrs.getOrCreateIntegerAttribute('fixed');
  for (Index row = 0; row <= adjRows; ++row)
    fixedAttr.values[row * (adjCols + 1)] = true;
}
