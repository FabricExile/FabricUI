            "code" : "require Math;

operator _Attributes_MathGeometry_SurfaceAngle<<<i>>>(
  io Ref<ScalarAttribute> result,
  io PolygonMesh mesh,
  Vec3 positions[],
  Vec3 normals[],
  Scalar minValue,
  Scalar maxValue)
{
  UInt32 count = mesh.getPointPolygonCount(i);
  if(count == 0 || minValue >= maxValue)
  {
    mesh.setPointAttribute(i, result, minValue);
    return;
  }

  UInt32 validCount = 0;
  if(count > 16)
    count = 16;
  Vec3 dirs[16];
  Vec3 normal;
  for(UInt32 k=0;k<16;k++)
  {
    if(k == count)
      break;
    UInt32 attrIndex = mesh.getPointAttributeIndex( i, k );
    normal += normals[attrIndex];
    Size polygon = 0;
    Size polygonPointIndex = 0;
    mesh.getPointPolygonAndIndex(i, k, polygon, polygonPointIndex);
    Size polygonSize = mesh.getPolygonSize(polygon);
    polygonPointIndex = (polygonPointIndex + 1) % polygonSize;
    attrIndex = mesh.getPolygonAttributeIndex(polygon, polygonPointIndex);

    dirs[k] = positions[i] - positions[attrIndex];
    if(!dirs[k].almostEqual(Vec3(0, 0, 0)))
    {
      dirs[k] = dirs[k].unit();
      validCount++;
    }
  }
  normal = normal.unit();

  Scalar overallAngle = 0.0;
  if(validCount > 0)
  {
    for(UInt32 k=0;k<16;k++)
    {
      if(k == count)
        break;
      if(dirs[k].almostEqual(Vec3(0, 0, 0)))
        continue;
      overallAngle = normal.angleTo(dirs[k]) / Scalar(validCount);
    }
  }

  Scalar v = Math_clamp(overallAngle, minValue, maxValue);
  v = (v - minValue) / (maxValue - minValue);
  mesh.setPointAttribute(i, result, v);
}

operator _Attributes_MathGeometry_Thickness<<<i>>>(
  io Ref<ScalarAttribute> result,
  io PolygonMesh mesh,
  Vec3 positions[],
  Vec3 normals[],
  Scalar minValue,
  Scalar maxValue)
{
  UInt32 count = mesh.getPointPolygonCount(i);
  if(count == 0 || minValue >= maxValue)
  {
    mesh.setPointAttribute(i, result, minValue);
    return;
  }

  Ray ray(positions[i] - normals[i] * 0.001, -normals[i]);
  GeometryLocation loc = mesh.raycast(ray, true, 0, SCALAR_INFINITE);
  if(!loc.isValid())
  {
    mesh.setPointAttribute(i, result, minValue);
    return;
  }

  Vec3 p = mesh.getPositionAtLocation(loc);
  Scalar v = Math_clamp((p - positions[i]).length(), minValue, maxValue);
  v = (v - minValue) / (maxValue - minValue);
  mesh.setPointAttribute(i, result, v);

}

operator _Attributes_MathGeometry_Distance<<<i>>>(
  io Ref<ScalarAttribute> result,
  io PolygonMesh mesh,
  io SpatialQueryable spatial,
  Vec3 positions[],
  Scalar minValue,
  Scalar maxValue)
{
  UInt32 count = mesh.getPointPolygonCount(i);
  if(count == 0 || minValue >= maxValue)
  {
    mesh.setPointAttribute(i, result, minValue);
    return;
  }

  GeometryLocation loc = spatial.getClosest(positions[i], Vec3(1, 1, 1), SCALAR_INFINITE);
  if(!loc.isValid())
  {
    mesh.setPointAttribute(i, result, minValue);
    return;
  }

  Vec3 p = spatial.getPositionAtLocation(loc);
  Scalar v = Math_clamp((p - positions[i]).length(), minValue, maxValue);
  v = (v - minValue) / (maxValue - minValue);
  mesh.setPointAttribute(i, result, v);
}

