      mat33Values = baseGeom.getMat33AttributeValueArray(attr, mode, indices, type);
      break;
    }
    case Mat44Attribute:
    {
      mat44Values = baseGeom.getMat44AttributeValueArray(attr, mode, indices, type);
      break;
    }
    case ColorAttribute:
    {
      colorValues = baseGeom.getColorAttributeValueArray(attr, mode, indices, type);
      break;
    }
    case RGBAttribute:
    {
      rgbaValues = baseGeom.getRGBAAttributeValueArray(attr, mode, indices, type);
