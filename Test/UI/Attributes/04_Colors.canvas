  {
    report(\"Error: missing geometry in \\\"GetValueArray\\\"\");
    return;
  }

  GeometryAttributes attributes = baseGeom.attributes;
  GeometryAttribute attr = attributes.getAttribute(attributeName);
  if(attr == null)
    return;

  Index idx = 0;
  switch(attr.type())
  {
    case IntegerAttribute:
    {
      sint32Values = baseGeom.getIntegerAttributeValueArray(attr, mode, indices, type);
      break;
    }
    case UInt32Attribute:
    {
      sint32Values = baseGeom.getIntegerAttributeValueArray(attr, mode, indices, type);
      break;
    }
    case ScalarAttribute:
    {
      float32Values = baseGeom.getScalarAttributeValueArray(attr, mode, indices, type);
      break;
    }
    case Vec2Attribute:
    {
      vec2Values = baseGeom.getVec2AttributeValueArray(attr, mode, indices, type);
