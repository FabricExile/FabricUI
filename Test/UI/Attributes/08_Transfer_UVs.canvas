  success = false;
  if(sourceAttr == \"\")
    return;
  if(targetAttr == \"\")
    return;

  BaseGeometry sourceBaseGeom = sourceGeometry;
  if(sourceBaseGeom == null)
    return;

  BaseGeometry targetBaseGeom = targetGeometry;
  if(targetBaseGeom == null)
    return;

  success = targetBaseGeom.transferAttributeFrom(
    sourceBaseGeom,
    mode,
    closestIfNotHit,
    edgeThreshold,
    component,
    sourceAttr,
    targetAttr,
    sourceXfo,
    targetXfo,
    direction
  );
}
