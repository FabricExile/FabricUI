        ],
      "definition" : {
        "objectType" : "Func",
        "title" : "calculateCoolColors",
        "ports" : [
          {
            "objectType" : "Port",
            "nodePortType" : "IO",
            "name" : "exec",
            "execPortType" : "IO",
            "typeSpec" : "Execute"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "positions",
            "execPortType" : "In",
            "typeSpec" : "Vec3[]"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "normals",
            "execPortType" : "In",
            "typeSpec" : "Vec3[]"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "IO",
            "name" : "colors",
            "execPortType" : "IO",
            "typeSpec" : "Color[]"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "waterColor",
            "execPortType" : "In",
            "typeSpec" : "Color"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "terrainColor",
            "execPortType" : "In",
            "typeSpec" : "Color"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "snowColor",
            "execPortType" : "In",
            "typeSpec" : "Color"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "waterLimit",
            "execPortType" : "In",
            "typeSpec" : "Scalar"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "snowLimit",
            "execPortType" : "In",
            "typeSpec" : "Scalar"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "snowLimitSoftness",
            "execPortType" : "In",
            "typeSpec" : "Scalar"
            }
          ],
        "extDeps" : {},
        "code" : "dfgEntry
{
  //
  // note: this could be multithreaded using PEX,
  // but for simplicity we will just use regular,
  // singlethreaded for-loops.
  //

  // make sure all the arrays have the same size.
  if (   positions.size() == normals.size()
      && positions.size() == colors.size() )
  {
    for (UInt32 i=0;i<positions.size();i++)
    {
      if (positions[i].y <= waterLimit)
      {
        // we are below the water limit,
        // so we simply use the water color.
        colors[i] = waterColor;
      }
      else
      {
        // fade between the terrain and the
        // snow color depending on how much
        // the normal points upwards.
        Scalar snowMin = snowLimit - snowLimitSoftness;
        Scalar snowMax = snowLimit + snowLimitSoftness;
        Scalar fade = (normals[i].y > snowLimit ? 1 : 0);
        if (snowMin < snowMax)
          fade = (normals[i].y - snowMin) / (snowMax - snowMin);
        if (fade < 0)  fade = 0;
        if (fade > 1)  fade = 1;
        colors[i] = terrainColor.linearInterpolate(snowColor, fade);
      }
    }
  }
}
