      "code" : "require AttributeHelpers;

dfgEntry {
  success = false;
  if(attributeName == \"\")
  {
    report(\"Error: empty attribute name in \\\"SetValue\\\"\");
    return;
  }

  BaseGeometry baseGeom = geometry;
  if(baseGeom == null)
  {
    report(\"Error: missing geometry in \\\"SetValue\\\"\");
    return;
  }

  GeometryAttributes attributes = baseGeom.attributes;
  GeometryAttribute attr = attributes.getAttribute(attributeName);
  if(attr == null)
    return;
  
  switch(attr.type())
  {
    case IntegerAttribute:
    {
      success = baseGeom.setIntegerAttributeValue(attr, mode, index, faceCorner, sint32Value);
      break;
    }
    case UInt32Attribute:
    {
      success = baseGeom.setUInt32AttributeValue(attr, mode, index, faceCorner, uint32Value);
      break;
    }
    case ScalarAttribute:
    {
      success = baseGeom.setScalarAttributeValue(attr, mode, index, faceCorner, float32Value);
      break;
    }
    case Vec2Attribute:
    {
      success = baseGeom.setVec2AttributeValue(attr, mode, index, faceCorner, vec2Value);
      break;
    }
    case Vec3Attribute:
    {
      success = baseGeom.setVec3AttributeValue(attr, mode, index, faceCorner, vec3Value);
      break;
    }
    case Vec4Attribute:
    {
      success = baseGeom.setVec4AttributeValue(attr, mode, index, faceCorner, vec4Value);
      break;
    }
    case QuatAttribute:
    {
      success = baseGeom.setQuatAttributeValue(attr, mode, index, faceCorner, quatValue);
      break;
    }
    case Mat33Attribute:
    {
      success = baseGeom.setMat33AttributeValue(attr, mode, index, faceCorner, mat33Value);
      break;
    }
    case Mat44Attribute:
    {
      success = baseGeom.setMat44AttributeValue(attr, mode, index, faceCorner, mat44Value);
      break;
    }
    case ColorAttribute:
    {
      success = baseGeom.setColorAttributeValue(attr, mode, index, faceCorner, colorValue);
      break;
    }
    case RGBAttribute:
    {
      success = baseGeom.setRGBAAttributeValue(attr, mode, index, faceCorner, rgbaValue);
      break;
    }
    case RGBAAttribute:
    {
      success = baseGeom.setRGBAttributeValue(attr, mode, index, faceCorner, rgbValue);
      break;
    }
    case UInt16ConstantArrayAttribute:
    {
      success = baseGeom.setUInt16ConstantArrayAttributeValue(attr, mode, index, faceCorner, uint16ArrayValue);
      break;
    }
    case ScalarConstantArrayAttribute:
    {
      success = baseGeom.setScalarConstantArrayAttributeValue(attr, mode, index, faceCorner, float32ArrayValue);
      break;
    }
    case SkinningAttribute:
    {
      success = baseGeom.setSkinningAttributeValue(attr, mode, index, faceCorner, skinIdValue, skinWeightValue);
      break;
    }
  }
}
